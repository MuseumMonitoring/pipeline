@prefix cidoc: <http://www.cidoc-crm.org/cidoc-crm/>.
@prefix sosa: <http://www.w3.org/ns/sosa/>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix p-plan: <http://purl.org/net/p-plan#>.
@prefix prov: <http://www.w3.org/ns/prov#>.
@prefix sds: <https://w3id.org/sds#>.
@prefix dcat: <https://www.w3.org/ns/dcat#>.
@prefix ex: <http://example.org/ns#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix void: <http://rdfs.org/ns/void#>.
@prefix ldes: <https://w3id.org/ldes#>.
@prefix : <http://me#>.

:dataset a dcat:Dataset;
  dcat:title "MuMo Sensors";
  dcat:publisher [ foaf:name "Arthur Vercruysse" ];
  ldes:timestampPath <http://def.isotc211.org/iso19156/2011/Observation#OM_Observation.resultTime>;
  dcat:identifier <http://localhost:3000/ldes>.

# http://www.cidoc-crm.org/cidoc-crm/P53_has_former_or_current_location
<Platform> a sh:NodeShape;
  sh:targetClass sosa:Platform;
  sh:property [
    sh:path sosa:hosts;
    sh:node [ sh:property [
      sh:path sosa:observes;
      sh:node [ ];
    ] ];
  ].

<http://data.mumo.be/streams/sensors/by-location> a sds:Stream;
  p-plan:wasGeneratedBy [
    a p-plan:Activity;
    rdfs:comment "Load in MuMo sensor data";
  ];
  sds:shape <Platform>;
  sds:carries [ a sds:Member ];
  sds:dataset :dataset.

