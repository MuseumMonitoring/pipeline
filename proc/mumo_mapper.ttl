@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix js: <https://w3id.org/conn/js#>.
@prefix fno: <https://w3id.org/function/ontology#>.
@prefix fnom: <https://w3id.org/function/vocabulary/mapping#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix : <https://w3id.org/conn#>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdfc: <https://w3id.org/rdf-connect#>.

rdfc:MumoMapper a owl:Class, rdfs:Class;
  rdfc:jsImplementationOf rdfc:Processor;
  rdfs:label "Mumo Mapper";
  rdfs:comment "Maps incoming JSON objects in MuMo measurements.";
  # rdfc:file <../lib/src/mapper.js>;
  rdfc:file <../lib/mapper2.js>;
  rdfc:class "Mapper";
  rdfc:entrypoint <../>.

#
# [ ] a sh:NodeShape;
#   sh:targetClass rdfc:MumoMapper;
#   sh:property [
#     sh:class rdfc:Writer;
#     sh:path rdfc:dataOutput;
#     sh:name "writer";
#     sh:maxCount 1;
#     sh:minCount 1;
#   ], [
#     sh:class rdfc:Reader;
#     sh:path rdfc:dataInput;
#     sh:name "reader";
#     sh:maxCount 1;
#     sh:minCount 1;
#   ].
[ ] a sh:NodeShape;
  sh:targetClass <Data>;
  sh:property [
    sh:class rdfc:Writer;
    sh:path rdfc:writer;
    sh:name "writer";
    sh:maxCount 1;
    sh:minCount 1;
  ], [
    sh:class rdfc:Reader;
    sh:path rdfc:reader;
    sh:name "reader";
    sh:maxCount 1;
    sh:minCount 1;
  ].

[ ] a sh:NodeShape;
  sh:targetClass rdfc:MumoMapper;
  sh:property [
    sh:class rdfc:Writer;
    sh:path rdfc:trigger;
    sh:name "trigger";
    sh:maxCount 1;
    sh:minCount 1;
  ], [
    sh:class <Data>;
    sh:path rdfc:data;
    sh:name "data";
    sh:maxCount 1;
    sh:minCount 1;
  ], [
    sh:class <Data>;
    sh:path rdfc:sensor;
    sh:name "sensor";
    sh:maxCount 1;
    sh:minCount 1;
  ], [
    sh:datatype xsd:string;
    sh:path rdfc:groups;
    sh:name "groups";
    sh:maxCount 1;
    sh:minCount 1;
  ].

