@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix rdfc: <https://w3id.org/rdf-connect#>.
@prefix fno: <https://w3id.org/function/ontology#>.
@prefix fnom: <https://w3id.org/function/vocabulary/mapping#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix : <https://w3id.org/conn#>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.

rdfc:MumoFetch a owl:Class, rdfs:Class;
  rdfc:jsImplementationOf rdfc:Processor;
  rdfs:label "Mumo Fetch";
  rdfs:comment "Fetches everything following link headers";
  rdfc:file <../lib/fetch.js>;
  rdfc:class "Fetcher";
  rdfs:entrypoint <../>.

[ ] a sh:NodeShape;
  sh:targetClass rdfc:MumoFetch;
  sh:property [
    sh:class rdfc:Reader;
    sh:path rdfc:delay;
    sh:name "delayed";
    sh:maxCount 1;
  ], [
    sh:class rdfc:Writer;
    sh:path rdfc:dataOutput;
    sh:name "writer";
    sh:minCount 1;
    sh:maxCount 1;
  ], [
    sh:datatype xsd:string;
    sh:path rdfc:startUrl;
    sh:name "start_url";
    sh:minCount 1;
    sh:maxCount 1;
  ], [
    sh:datatype xsd:string;
    sh:path rdfc:savePath;
    sh:name "save_path";
    sh:maxCount 1;
  ], [
    sh:datatype xsd:integer;
    sh:path rdfc:intervalMs;
    sh:name "interval_ms";
    sh:maxCount 1;
  ].

