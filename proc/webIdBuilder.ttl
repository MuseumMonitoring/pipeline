@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix rdfc: <https://w3id.org/rdf-connect#>.
@prefix fno: <https://w3id.org/function/ontology#>.
@prefix fnom: <https://w3id.org/function/vocabulary/mapping#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix : <https://w3id.org/conn#>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.

rdfc:WebIdBuilder a owl:Class, rdfs:Class;
  rdfc:jsImplementationOf rdfc:Processor;
  rdfs:label "Builds webIds on request";
  rdfs:comment "Interacts with a css endpoint and returns a WebId and client credential on request";
  rdfc:file <../lib/webIdBuilder.js>;
  rdfc:class "WebIdBuilder";
  rdfc:entrypoint <../>.

[ ] a sh:NodeShape;
  sh:targetClass rdfc:WebIdBuilder;
  sh:property [
    sh:datatype xsd:string;
    sh:path rdfc:accountRoot;
    sh:name "accountRoot";
    sh:maxCount 1;
    sh:minCount 1;
  ], [
    sh:datatype xsd:string;
    sh:path rdfc:oidcEndpoint;
    sh:name "oidcEndpoint";
    sh:maxCount 1;
    sh:minCount 1;
  ], [
    sh:datatype xsd:integer;
    sh:path rdfc:port;
    sh:name "port";
    sh:maxCount 1;
    sh:minCount 1;
  ], [
    sh:datatype xsd:integer;
    sh:path rdfc:timeout;
    sh:name "timeout";
    sh:maxCount 1;
    sh:minCount 1;
  ].

